params=rv:$a,rv:$b
work=$x,$y,$c,$d
---
cmp $a,$b
jb noswap
set $x,$b
set $y,$a
jmp weiter
:noswap
set $x,$a
set $y,$b
:weiter
set $c,$x
set $d,$y
set $y,0
:loop
cmp $c,0
je fertig
sub $c,1
add $y,$d
jmp loop
:fertig
set $a,$y


